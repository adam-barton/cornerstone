{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\r\n\r\n    <li class=\"navUser-item\">\r\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\"><i class=\"icon\" aria-hidden=\"false\"><img alt=\"dollar sign icon\" src=\"{{cdn 'img/icon-currency.svg'}}\" width=\"34\" height=\"34\"></i></a>\r\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n            {{#each currency_selector.currencies}}\r\n            <li class=\"dropdown-menu-item\">\r\n                <a href=\"{{{switch_url}}}\">\r\n                    {{#if is_active}}\r\n                    <strong>{{name}}</strong>\r\n                    {{else}}\r\n                    {{name}}\r\n                    {{/if}}\r\n                </a>\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n    </li>\r\n\r\n{{/if}}\r\n","components/common/navigation":"<nav class=\"navUser\">\r\n       \r\n    <ul class=\"navUser-section navUser-section--alt\">\r\n          {{> components/common/currency-selector}}\r\n\r\n          <!-- <li class=\"navUser-item gift\">\r\n                <a class=\"navUser-action\" href=\"tel:660-886-9570\"> Call <img src=\"{{cdn 'img/tele.png'}}\" alt=\"Call\"/></a>\r\n            </li> -->\r\n        \r\n     <!-- {{#if customer.store_credit.value '>' 0}}\r\n               <li class=\"navUser-item\">\r\n                <a class=\"navUser-action navUser-action--storeCredit\"\r\n                   data-dropdown=\"storeCredit-dropdown\"\r\n                   data-options=\"align:bottom\"\r\n                   href=\"{{urls.cart}}\">\r\n                        <span class=\"navUser-action-divider\">{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\r\n                </a>\r\n                <div class=\"dr